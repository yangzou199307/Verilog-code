    %vpi_call 2 4 "$display", "hello world by yangz!" {0 0 0};
    %vpi_call 2 5 "$dumpfile", "mytest.vcd" {0 0 0};
    %vpi_call 2 6 "$dumpvars" {0 0 0};
    %delay 10, 0;
    %vpi_call 2 7 "$finish" {0 0 0};
    %end;
    .thread T_0;
